{
  "builds": [
    { "src": "UI/**/*", "use": "@vercel/static" },
    { "src": "Backend/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "Backend/server.js" },
    { "src": "/(.*)", "dest": "UI/$1" }

  ],
  "env": {
    "API_URL": "http://localhost:5000/api/compatibility"
  }
}

